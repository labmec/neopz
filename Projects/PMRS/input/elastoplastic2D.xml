<?xml version="1.0" ?>
    <!-- Example 1: Simulating an poroelastic example  -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/manouchehr/Documents/GitHub/neopz/Projects/PMRS/input/meshes/elastoplastic2D.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="1.0e-6" />
        <StepNumber n_time_steps="10" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="10" />
        <Residue res_tolerance="0.001" />
        <Correction cor_tolerance="0.00001" />
    </NewtonControls> 
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="4" />
    </ParallelControls> 
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="elastoplastic2D.vtk" n_divisions="0" n_scalars="10" n_vectors="1"
        />
        <Scalars>
            <Var name="s_x" />
            <Var name="s_y" />
            <Var name="t_xy" />
            <Var name="p" />
            <Var name="e_x" />
            <Var name="e_y" />
            <Var name="e_xy" />
            <Var name="ep_x" />
            <Var name="ep_y" />
            <Var name="ep_xy" />
        </Scalars>
        <Vectors>
            <Var name="u" />
        </Vectors>
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
        <Prestress sxx= "0.0" syy= "-6.0e6" szz= "0.0" sxy= "0.0" sxz= "0.0" syz= "0.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries="3" n_parameters="8" >
            <Parameters eyoung="4.36e9" nu="0.2" phi="0.25" kappa="1.0e-14" alpha="0.7" m="1.0e-7" rho="1000.0" mu="0.001" />
            <Boundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </Boundaries>
        </RegionData>          
    </RegionsDefinition>
    <BoundaryConditions>
        <BoundaryCondition bc_id="2" type="Duy_Nq" uy="0.0" qn="0.0" />
        <BoundaryCondition bc_id="3" type="Ntn_time_Dp" tx="0.0" ty="-2.0e6" p="0.0" />
        <BoundaryCondition bc_id="4" type="Dux_Nq" ux="0.0" qn="0.0" />
    </BoundaryConditions>           
</CaseData>
