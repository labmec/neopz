<?xml version="1.0" ?>
    <!-- Example 1: Simulating an Terzaghi poroelastic  -->
<CaseData>
    <!-- Geometry description -->
    <Mesh>
       <MeshFile mesh_file="/Users/omar/Documents/GitHub/neopz/Projects/PoropermCoupling/input/meshes/Terzaghi.msh" />
    </Mesh>
    <!-- Time step controls  -->
    <TimeControls>
        <StepSize dt="0.001" />
        <StepNumber n_time_steps="10" />
    </TimeControls>
    <!-- Newton method controls  -->
    <NewtonControls>
        <Iterations n_iterations="1" />
        <Residue res_tolerance="0.01" />
        <Correction cor_tolerance="0.01" />
    </NewtonControls>
    <!-- Parallel controls  -->
    <ParallelControls>
        <Numthreads n_threads="4" />
    </ParallelControls>  
    <!-- Finite element controls  -->
    <FEM>
        <MixedFormulationQ useQ="0" />
        <HRefine h_level="0" />
        <Elasticity p_order="2" />
        <Diffusion p_order="1" />
    </FEM>
    <!-- Settings for OutPut -->
    <OutputControls>
        <OutputFolder name="mkdir Output" />
        <LogFolder name="mkdir Log" />
        <PostProcessing vtk_file="Terzaghi.vtk" n_divisions="0" n_scalars="24" n_vectors="2"
        />
        <Scalars>
            <Var name="s_x" />
            <Var name="s_y" />
            <Var name="s_z" />
            <Var name="t_xy" />
            <Var name="t_xz" />
            <Var name="t_yz" />
            <Var name="p" />
            <Var name="k_x" />
            <Var name="k_y" />
            <Var name="k_z" />
            <Var name="phi" />
            <Var name="e_x" />
            <Var name="e_y" />
            <Var name="e_z" />
            <Var name="e_xy" />
            <Var name="e_xz" />
            <Var name="e_yz" />
            <Var name="ep_x" />
            <Var name="ep_y" />
            <Var name="ep_z" />
            <Var name="ep_xy" />
            <Var name="ep_xz" />
            <Var name="ep_yz" />
            <Var name="K_0" />
        </Scalars>
        <Vectors>
            <Var name="u" />
            <Var name="v" />
        </Vectors>
    </OutputControls>
    <Physics>
        <GravityConstant gravity="0.0" />
        <GravityDirection x_direction="0.0" y_direction="0.0" z_direction="-1.0" />
        <Prestress sxx= "0.0" syy= "0.0" szz= "0.0" sxy= "0.0" sxz= "0.0" syz= "0.0" />
    </Physics>
    <!-- Material Settings -->
    <ReservoirRegions>
        <RegionNumber n_regions="1" />
    </ReservoirRegions> 
    <RegionsDefinition>
        <!-- Region definition -->
        <RegionData mat_id="1" n_boundaries="3" n_parameters="8" >
            <Parameters eyoung="0.9" nu="0.2" phi="1.0" kappa="1.0" alpha="1.0" m="1.0e29" rho="1.0" mu="1.0" />
            <Boundaries>
                <Boundary bc_id="2"/>
                <Boundary bc_id="3"/>
                <Boundary bc_id="4"/>
            </Boundaries>
        </RegionData>          
    </RegionsDefinition>
    <BoundaryConditions>
        <BoundaryCondition bc_id="2" type="Duy_Nq" uy="0.0" qn="0.0" />
        <BoundaryCondition bc_id="3" type="Nt_Dp" tx="0.0" ty="-1.0" p="0.0" />
        <BoundaryCondition bc_id="4" type="Dux_Nq" ux="0.0" qn="0.0" />
    </BoundaryConditions>           
</CaseData>
